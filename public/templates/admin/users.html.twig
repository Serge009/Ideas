{% extends "base.html.twig" %}
{% block inner %}
    <table>
        <thead>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Login</th>
            <th>Password</th>
            <th>Type</th>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.name }}</td>
                <td>{{ user.surname }}</td>
                <td>{{ user.login }}</td>
                <td>{{ user.pass }}</td>
                <td>{{ user.type.type }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}